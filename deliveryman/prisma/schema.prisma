// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Deliveryman {
  id       Int     @id @default(autoincrement())
  name     String
  email    String
  password String
  isOnline Boolean @default(false)
  lat      Float
  lng      Float

  @@map("tbl_deliveryman")
}
